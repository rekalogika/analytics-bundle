{
    "name": "@rekalogika/analytics-bundle",
    "description": "TBD",
    "license": "MIT",
    "version": "0.0.1",
    "symfony": {
        "controllers": {
            "pivot-table": {
                "main": "dist/pivot-table-controller.js",
                "fetch": "eager",
                "enabled": true
            },
            "equal-filter": {
                "main": "dist/equal-filter-controller.js",
                "fetch": "eager",
                "enabled": true,
                "autoimport": {
                    "tom-select/dist/css/tom-select.default.css": true,
                    "tom-select/dist/css/tom-select.bootstrap4.css": false,
                    "tom-select/dist/css/tom-select.bootstrap5.css": false
                }
            },
            "date-range-filter": {
                "main": "dist/date-range-controller.js",
                "fetch": "eager",
                "enabled": true,
                "autoimport": {
                    "flatpickr/dist/flatpickr.css": true
                }
            }
        },
        "importmap": {
            "@hotwired/stimulus": "^3.0",
            "@hotwired/turbo": "^7.0",
            "sortablejs": "^1.14.0",
            "tom-select": "^2.4.3",
            "flatpickr": "^4.6.13"
        }
    },
    "scripts": {
        "build": "babel src -d dist"
    },
    "peerDependencies": {
        "@hotwired/stimulus": "^3.0.0",
        "sortablejs": "^1.14.0",
        "tom-select": "^2.4.3",
        "flatpickr": "^4.6.13"
    },
    "devDependencies": {
        "@babel/cli": "^7.26",
        "@babel/core": "^7.26",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/preset-env": "^7.26.0",
        "@hotwired/stimulus": "^3.0",
        "@hotwired/turbo": "^7.0"
    }
}